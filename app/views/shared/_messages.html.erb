<div id="messages">
<% flash.each do |name, msg| %>
    <div class="alert alert-dismissable alert-<%= name %>" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <%= msg %>
    </div>
<% end %>
</div>
